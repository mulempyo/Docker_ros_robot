<?xml version="1.0"?>
<robot name="KWJBot" xmlns:xacro="http://ros.org/wiki/xacro">
    <xacro:include filename="$(find kwj_description)/urdf/kwj.gazebo.xacro"/>

    <link name="base_footprint"/>

    <link name="base_link">
      <inertial>
        <origin xyz="0 0 0.065" rpy="0 0 0"/>
        <mass value="0.8343"/> 
        <inertia ixx="0.005259" ixy="0.005470" ixz="0.003567"
                 iyy="0.005710" iyz="0.005603"
                 izz="0.005524" />
        </inertial>

      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <box size="0.155 0.2 0.15"/>
        </geometry>
      </collision>

      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://kwj_description/meshes/base_link.stl" scale="1.0 1.0 1.0"/>
        </geometry>
        <material name="grey">
          <color rgba="0.5 0.5 0.5 1"/>
        </material>
      </visual>
    </link>

    <joint name="base_joint" type="fixed">
         <parent link="base_footprint"/>
         <child link="base_link"/>
         <origin xyz="0 0 0.065" rpy="0 0 0"/> 
      </joint>

    <joint name="camera_joint" type="fixed">
      <parent link="base_link"/>
      <child link="camera_link"/>
      <origin xyz="0.15 0 0.115" rpy="0 0 0"/> 
    </joint>

    <link name="camera_link">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://kwj_description/meshes/camera_gazebo.stl" scale="1.0 1.0 1.0"/>
        </geometry>
        <material name="grey">
          <color rgba="0.5 0.5 0.5 1"/>
        </material>
      </visual>
      <collision>
        <origin xyz="0.015 0 -0.0065" rpy="0 0 0"/>
        <geometry>
          <box size="0.05 0.05 0.025"/> 
        </geometry>
      </collision>
      <inertial>
        <mass value="3.07365e-05"/> 
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <inertia ixx="3.073649974518707e-05" iyy="8.101563506798558e-06" izz="4.316406649983658e-05" ixy="0" ixz="0" iyz="0"/>
      </inertial>
    </link>

     <joint name="wheel_left_joint" type="continuous">
      <parent link="base_link"/>
      <child link="wheel_left_link"/>
      <origin xyz="-0.0355 0.106 -0.0515" rpy="-1.57 0 0"/>
      <axis xyz="0 0 1"/>
     </joint>

     <link name="wheel_left_link">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 1.57"/>
          <geometry>
           <cylinder length="0.026" radius="0.0335"/>
          </geometry>
        <material name="dark"/>
      </visual>

      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <cylinder length="0.026" radius="0.0335"/>
        </geometry>
      </collision>

      <inertial>
        <origin xyz="0 0 0" />
        <mass value="0.1004" />
        <inertia ixx="3.3800152140578693e-05" ixy="0" ixz="0"
                 iyy="5.628900326021782e-05"  iyz="0"
                 izz="3.380015380915577e-05" />
       </inertial>
    </link>

     <joint name="wheel_right_joint" type="continuous">
      <parent link="base_link"/>
      <child link="wheel_right_link"/>
      <origin xyz="-0.0355 -0.106 -0.0515" rpy="-1.57 0 0"/>
      <axis xyz="0 0 1"/>
     </joint>

     <link name="wheel_right_link">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 1.57"/>
         <geometry>
           <cylinder length="0.026" radius="0.0335"/>
         </geometry>
        <material name="dark"/>
      </visual>

      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <cylinder length="0.026" radius="0.0335"/>
        </geometry>
      </collision>

      <inertial>
        <origin xyz="0 0 0" />
        <mass value="0.1004" />
        <inertia ixx="3.3800152140578693e-05" ixy="0" ixz="0"
                 iyy="5.628900326021782e-05" iyz="0"
                 izz="3.380015380915577e-05" />
        </inertial>
    </link> 

    <joint name="caster_front_joint" type="fixed">
      <parent link="base_link"/>
      <child link="caster_front_link"/>
      <origin xyz="0.14 0 -0.07" rpy="-1.57 0 0"/>
    </joint>

    <link name="caster_front_link">

      <visual>
        <origin xyz="0 0 0" rpy="-1.57 0 -1.57"/>
        <geometry>
          <mesh filename="package://kwj_description/meshes/caster_gazebo.stl" scale="1.0 1.0 1.0"/>
        </geometry>
        <material name="grey">
          <color rgba="0.5 0.5 0.5 1"/>
        </material>
      </visual>

      <collision>
        <origin xyz="0 0.001 0" rpy="0 0 0"/>
        <geometry>
          <cylinder length="0.016" radius="0.015"/>
        </geometry>
      </collision>

      <inertial>
        <origin xyz="0 0 0" />
        <mass value="0.01439" />
        <inertia ixx="1.21919e-06" ixy="1.70883e-06" ixz="1.21919e-06"
                 iyy="2.19848e-06" iyz="1.70883e-06"
                 izz="1.21919e-06" />
      </inertial>
    </link>

     <joint name="imu_joint" type="fixed">
      <parent link="base_link"/>
      <child link="imu_link"/>
      <origin xyz="-0.07 0 0" rpy="0 0 0"/>
     </joint>

     <link name="imu_link"/>
    
     <joint name="scan_joint" type="fixed">
      <parent link="base_link"/>
      <child link="laser_link"/>
      <origin xyz="0.11 0 0.18" rpy="0 0 0"/>
     </joint>

     <link name="laser_link">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://kwj_description/meshes/lidar_gazebo.stl" scale="1.0 1.0 1.0"/>
        </geometry>
        <material name="grey">
          <color rgba="0.5 0.5 0.5 1"/>
        </material>
      </visual>

      <collision>
        <origin xyz="-0.015 0 -0.0065" rpy="0 0 0"/>
        <geometry>
          <cylinder length="0.0105" radius="0.03435"/>
        </geometry>
      </collision>

      <inertial>
        <mass value="0.04659" />
        <origin xyz="0 0 0" />
        <inertia ixx="1.41344e-05" ixy="0" ixz="0"
                 iyy="1.40144e-05" iyz="0"
                 izz="2.74129e-05" />
      </inertial>
    </link>

</robot>
